@model IEnumerable<App.Model.Order>
<h1>Hello</h1>
<table>
    <thead>
        <tr>
            <th>Date</th>
            <th>Total</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model){
            <tr>
            <th>@item.OrderDate</th>
            <th>@item.OrderTotal</th>
        </tr>
        }
<th></th>
    </tbody>
</table>